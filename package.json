{
  "name": "ColoCalc",
  "description": "Une application pour mieux gérer vos dépenses entre colocataires",
  "author": "Samir KHERRAZ <samir.kherraz@outlook.fr>",
  "version": "1.0.2",
  "repository": "https://github.com/samirkherraz/colocalc",
  "keywords": [
    "Colocataires",
    "Dépenses",
    "Gérer"
  ],
  "devDependencies": {
    "electron-prebuilt": "latest"
  },
  "license": "GPLv3",
  "main": "index.js",
  "private": false,
  "scripts": {
    "start": "electron ."
  },
  
  "build": {
    "files": [
      "**/*",
      "!.git",
      "!node_modules",
      "!dist",
      "build.sh",
      "package.json"
    ],
    "compression": "maximum",
    "appId": "com.skad.colocalc",
    "linux": {
      "icon": "com.skad.colocalc/icons/",
      "target": [
        "AppImage",
        "deb",
        "rpm"
        
      ],
      "maintainer": "Samir KHERRAZ <samir.kherraz@outlook.fr"
    }
  }
}
